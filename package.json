{
  "name": "moufette",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "front": "react-app-rewired start",
    "start": "node server",
    "build": "react-app-rewired build ",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "copy-widget": "mkdir -p public/widget/ && cp node_modules/moufette-widget/build/static/js/main.* public/widget/",
    "postinstall": "yarn copy-widget && yarn min-js-loader",
    "min-js-loader": "uglifyjs src/scripts/js-loader.js -o src/scripts/js-loader.min.txt -c -m",
    "set-heroku-env": "cat .env.production | tr '\n' ' ' | xargs heroku config:set"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.43",
    "@slack/web-api": "^5.9.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.4",
    "@types/styled-components": "^5.1.0",
    "@types/webpack-env": "^1.15.1",
    "antd": "^4.2.4",
    "apollo-boost": "^0.4.7",
    "apollo-server": "^2.12.0",
    "axios": "^0.19.2",
    "babel-plugin-import": "^1.13.0",
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "customize-cra": "^0.9.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.17.0",
    "graphql": "^15.0.0",
    "graphql-passport": "^0.6.3",
    "graphql-tools": "^5.0.0",
    "immer": "^6.0.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.9",
    "moufette-widget": "https://github.com/moufette-tools/moufette-widget.git",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.5",
    "react-browser-frame": "^1.0.9",
    "react-color": "^2.18.1",
