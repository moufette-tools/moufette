    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "ssl-express-www": "^3.0.6",
    "styled-components": "^5.1.0",
    "typescript": "~3.7.2",
    "uglify-js": "^3.9.1"
  },
  "proxy": "http://localhost:5000/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
